<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CountDownProgressBar">
        <attr name="lctyText" format="string"></attr>
        <attr name="lctyColor" format="color"></attr>
        <attr name="lctySize" format="dimension"></attr>

        <attr name="countDown_circleWidth" format="dimension" />
        <attr name="countDown_centerTextSize" format="dimension" />
        <attr name="countDown_betaAngle" format="integer" />
        <attr name="countDown_firstColor" format="color" />
        <attr name="countDown_secondColor" format="color" />
        <attr name="countDown_centerTextColor" format="color" />
        <attr name="countDown_isShowGradient" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MyEditText">
        <attr name="TextType" format="integer"/>
        <attr name="PasteId" format="reference"/>
        <attr name="DisplayStyle" format="integer"/>
    </declare-styleable>

    <declare-styleable name="WheelPicker">
        <attr name="halfVisibleItemCount"/>
        <attr name="itemTextSize"/>
        <attr name="itemMaximumWidthText"/>
        <attr name="itemTextColor"/>
        <attr name="textGradual"/>
        <attr name="selectedTextColor"/>
        <attr name="selectedTextSize"/>
        <attr name="currentItemPosition"/>
        <attr name="itemWidthSpace"/>
        <attr name="itemHeightSpace"/>
        <attr name="zoomInSelectedItem"/>
        <attr name="wheelCyclic"/>
        <attr name="wheelCurtain"/>
        <attr name="wheelCurtainColor"/>
        <attr name="wheelCurtainBorder" />
        <attr name="wheelCurtainBorderColor"/>
        <attr name="indicatorText"/>
        <attr name="indicatorTextSize"/>
        <attr name="indicatorTextColor"/>
    </declare-styleable>

    <declare-styleable name="AutoLineUtil">
        <attr name="horizontalSpacing" format="dimension"/>
        <attr name="verticalSpacing" format="dimension"/>
    </declare-styleable>

</resources>


